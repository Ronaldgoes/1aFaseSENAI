programa {
  funcao inicio() {
    inteiro numero1, numero2

    escreva("Digite um numero: ")
    leia(numero1)

    escreva("Digite outro numero: ")
    leia(numero2)

    se(numero1 < numero2){
      escreva("Número2 é maior que número1 = ", numero2)
    }
    se(numero1 > numero2){
    escreva("Número1 é maior que número2 = ", numero1)

    }se(numero1 == numero2){
      escreva("Os números são iguais. ")
    }
    
    }
    
    
  }
}
